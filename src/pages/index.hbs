{{#extend "layout"}}
    {{#content "main"}}
        <section class="intro">
            <picture>
                <source srcset="img/intro-bg-mobile.jpg" media="(max-width: 420px)">
                <img class="intro__bg" src="img/intro-bg.jpg" alt="Умка">
            </picture>
            <div class="container">
                <div class="intro__inner">
                    <h1 class="intro__title main-title">Умка — Льдозаливочная машина для открытых арен </h1>
                    <div class="intro__footer">
                        <div class="intro__buttons">
                            <button class="intro__button-top button">
                                <span>Подберите УМКУ со скидкой 10%</span>
                                <svg>
                                    <use xlink:href="#gear-icon"></use>
                                </svg>
                            </button>
                            <button class="intro__button-bottom">
                                <svg class="intro__button-bottom-icon">
                                    <use xlink:href="#time-icon"></use>
                                </svg>
                                <span class="intro__button-bottom-text">предложение ограничено</span>
                                <span class="intro__button-bottom-timer">18:25:15</span>
                                <svg class="intro__button-bottom-bg">
                                    <use xlink:href="#timer-bg"></use>
                                </svg>
                            </button>
                        </div>
                        <div class="intro__slider intro-slider swiper-container">
                            <div class="swiper-wrapper">
                                <div class="intro-slider__slide swiper-slide">
                                    <p>Обслуживает до 30 катков в день</p>
                                    <div class="intro-slider__progressbar">
                                        <div class="intro-slider__progressbar-progress"></div>
                                    </div>
                                </div>
                                <div class="intro-slider__slide swiper-slide">
                                    <p>Для заливки и чистки льда на открытых катках</p>
                                    <div class="intro-slider__progressbar">
                                        <div class="intro-slider__progressbar-progress"></div>
                                    </div>
                                </div>
                                <div class="intro-slider__slide swiper-slide">
                                    <p>Для заливки и чистки льда на открытых катках</p>
                                    <div class="intro-slider__progressbar">
                                        <div class="intro-slider__progressbar-progress"></div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="intro__ribbon">
                            <p class="intro__ribbon-text">Лучшая альтернатива зарубежным аналогам</p>
                            <svg class="intro__ribbon-icon">
                                <use xlink:href="#rus-flag"></use>
                            </svg>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="stats">
            <div class="container">
                <div class="stats__inner">
                    <div class="stats__list-wrapper">
                        <ul class="stats__list">
                            <li class="stats__item-wrapper">
                                <div class="stats__item">
                                    <div class="stats__item-img">

                                    </div>
                                    <div class="stats__item-content">
                                        <h3 class="stats__item-title">
                                            <span>Умка решает любые поставленные задачи</span> от заливки льда, до мойки
                                            дорог
                                            и чистки снега
                                        </h3>
                                    </div>
                                </div>
                            </li>
                            <li class="stats__item-wrapper">
                                <div class="stats__item">
                                    <div class="stats__item-img">

                                    </div>
                                    <div class="stats__item-content">
                                        <div class="stats__item-text">
                                            <h3 class="stats__item-title">
                                                Вместительнее аналогов на
                                            </h3>
                                            <ul class="stats__item-stats">
                                                {{#each stats }}
                                                    <li>
                                                        <div class="stats__item-stat">
                                                            <span class="stats__item-stat-number">{{this.number}}</span>
                                                            <p class="stats__item-stat-text">{{this.text}}</p>
                                                        </div>
                                                    </li>
                                                {{/each}}
                                            </ul>
                                        </div>
                                        <div class="stats__item-number">
                                            <span>40%</span>
                                        </div>
                                    </div>
                                </div>
                            </li>
                            <li class="stats__item-wrapper">
                                <div class="stats__item">
                                    <div class="stats__item-img">

                                    </div>
                                    <div class="stats__item-content">
                                        <div class="stats__item-text">
                                            <h3 class="stats__item-title">
                                                Выгоднее аналогов на
                                            </h3>
                                            <div class="stats__item-benefit">
                                                <div class="stats__item-benefit-img">
                                                    <svg>
                                                        <use xlink:href="#bank-icon"></use>
                                                    </svg>
                                                </div>
                                                <div class="stats__item-benefit-text">
                                                    <p>
                                                        616-е постановление правительства дает преференции 15% в цене
                                                        при участии в аукционах
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="stats__item-number">
                                            <span>15%</span>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        </ul>
                        <div class="stats__img-wrapper">
                            <div class="stats__img-block">
                                <img class="stats__img stats-img-1" src="img/stat-1.png" alt="Фото преимущества">
                                <img class="stats__img stats-img-2" src="img/stat-2.png" alt="Фото преимущества">
                                <img class="stats__img stats-img-3" src="img/stat-3.png" alt="Фото преимущества">
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>
        <section class="features">
            <div class="container">
                <div class="features__wrapper">
                    <ul class="features__list">
                        {{#each features }}
                            <li class="features__item-wrapper">
                                <div class="features__item">
                                    <div class="features__item-img">
                                        <svg>
                                            <use xlink:href="#{{this.img}}"></use>
                                        </svg>
                                    </div>
                                    <div class="features__item-content">
                                        <h3 class="features__item-title">{{this.title}}</h3>
                                        <div class="features__item-text">
                                            <p>{{this.text}}</p>
                                        </div>
                                    </div>
                                </div>
                            </li>
                        {{/each}}
                    </ul>
                    <div class="features__img-slider">
                        <img src="img/features-slider-1.jpg" alt="Фото слайдера">
                    </div>
                </div>
            </div>
        </section>
        <section class="overview">
            <div class="wide-container">
                <div class="overview__inner">
                    <video class="overview__video" src="assets/video/umka.mp4" autoplay loop muted></video>
                    <div class="overview__video-billet">
                        {{> billet }}
                        <span class="overview__video-description">
                        Видеообзор эксперта <sup>9 минут</sup>
                    </span>
                    </div>
                </div>
            </div>
        </section>
        <section class="configure-poster">
            <div class="wide-container">
                <div class="configure-poster__wrapper">
                    <div class="configure-poster__text">
                        <h2>конфигурируется <span>разными типами оборудования</span></h2>
                    </div>
                    <div class="configure-poster__img">
                        <img src="img/configure-poster.png" alt="Разные типа оборудования">
                    </div>
                </div>
            </div>
        </section>
        <section class="configure">
            <div class="wide-container">
                <div class="configure__wrapper">
                    <div class="configure__graphic-column">
                        <h2 class="configure__title section-title">Сконфигурируйте свою умку</h2>
                        <svg class="configure__graphic">
                            <use xlink:href="#umka-model"></use>
                        </svg>
                    </div>
                    <div class="configure__text-column">
                        <ul class="configure__tabs">
                            <li class="configure__tab-wrapper">
                                <div class="configure__tab active">
                                    <div class="configure__tab-upperline"></div>
                                    <span class="configure__tab-text">На базе уаз</span>
                                </div>
                            </li>
                            <li class="configure__tab-wrapper">
                                <div class="configure__tab">
                                    <div class="configure__tab-upperline"></div>
                                    <span class="configure__tab-text">На базе isuzu</span>
                                </div>
                            </li>
                        </ul>
                        <form class="configure__form">
                            <div class="configure__form-fields">
                                <fieldset class="configure__form-group configure__form-group-chosen">
                                    <h3 class="configure__form-group-title">Выбранное оборудование</h3>
                                    <ul class="configure__chosen-equipments">
                                        <li class="configure__chosen-equipment-wrapper">
                                            <div class="configure__chosen-equipment">
                                                <span class="configure__chosen-equipment-text">Навеска</span>
                                                <button class="configure__chosen-equipment-delete">
                                                    <svg>
                                                        <use xlink:href="#plus-icon"></use>
                                                    </svg>
                                                </button>
                                            </div>
                                        </li>
                                        <li class="configure__chosen-equipment-wrapper">
                                            <div class="configure__chosen-equipment">
                                                <span class="configure__chosen-equipment-text">Отвал коммунальный</span>
                                                <button class="configure__chosen-equipment-delete">
                                                    <svg>
                                                        <use xlink:href="#plus-icon"></use>
                                                    </svg>
                                                </button>
                                            </div>
                                        </li>
                                        <li class="configure__chosen-equipment-wrapper">
                                            <button class="configure__chosen-equipment-more">
                                                еще 3
                                            </button>
                                        </li>
                                    </ul>
                                </fieldset>
                                <fieldset class="configure__form-group">
                                    <div class="configure__form-group-header">
                                        <h3 class="configure__form-group-title">Выбрать оборудование</h3>
                                        <button class="configure__form-group-button">
                                            <svg>
                                                <use xlink:href="#plus-icon"></use>
                                            </svg>
                                        </button>
                                    </div>
                                    <div class="configure__form-group-body">

                                    </div>
                                </fieldset>
                                <fieldset class="configure__form-group">
                                    <div class="configure__form-group-header">
                                        <h3 class="configure__form-group-title">Выбрать дополнительные опции</h3>
                                        <button class="configure__form-group-button">
                                            <svg>
                                                <use xlink:href="#plus-icon"></use>
                                            </svg>
                                        </button>
                                    </div>
                                    <div class="configure__form-group-body">

                                    </div>
                                </fieldset>
                            </div>
                            <div class="configure__form-price-block">
                                <div class="configure__form-price-wrapper">
                                    <span class="configure__form-price-description">Стоимость выбранной конфигурации от</span>
                                    <span class="configure__form-price">7 000 000 руб.</span>
                                </div>
                                <button class="configure__form-button button button--light">
                                    <span>Отправить заявку</span>
                                    <svg>
                                        <use xlink:href="#message-icon"></use>
                                    </svg>
                                </button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </section>
        <section class="expert">
        <div class="wide-container">
            <div class="expert__wrapper">
                <div class="expert__man">
                    <img src="img/person.png" alt="Профессионал">
                </div>
                <div class="expert__container-form">
                    <div class="expert__expert-title">
                        <h3>получите консультацию нашего эксперта</h3>
                    </div>
                    <form action="">
                        <div class="expert__item">
                            <input type="text" id="name"><label for="name">Ваше имя</label>
                        </div>
                        <div class="expert__item">
                            <input type="tel"><label for="name">ваш телефон</label>
                        </div>
                        <button type="submit"><span>Отправить</span>
                            <svg width="40" height="40" viewBox="0 0 40 40" fill="none"
                                xmlns="http://www.w3.org/2000/svg">
                                <path fill-rule="evenodd" clip-rule="evenodd"
                                    d="M17.469 31.1833L35.7807 21.4633C36.9624 20.8366 36.9624 19.15 35.7807 18.5216L17.4957 8.81665C16.1157 8.08331 14.569 9.46665 15.154 10.91L18.8024 19.91L15.124 29.0933C14.5474 30.5383 16.0924 31.9133 17.469 31.1833Z"
                                    stroke="#1B74E3" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
                                <path d="M18.8047 19.9167L36.6714 20" stroke="#1B74E3" stroke-width="2"
                                    stroke-linecap="round" stroke-linejoin="round" />
                                <path d="M6.67188 24.9998H9.33854" stroke="#1B74E3" stroke-width="2"
                                    stroke-linecap="round" stroke-linejoin="round" />
                                <path d="M5 19.9998H9.33333" stroke="#1B74E3" stroke-width="2" stroke-linecap="round"
                                    stroke-linejoin="round" />
                                <path d="M3.33594 14.9998H9.33594" stroke="#1B74E3" stroke-width="2"
                                    stroke-linecap="round" stroke-linejoin="round" />
                            </svg>


                        </button>
                    </form>
                    <div class="expert__agree">
                        <div class="expert__agree-star">*</div>
                        <div class="expert__agree-text">Нажимая кнопку «Отправить» вы соглашаетесь с политикой
                            конфиденциальности</div>

                    </div>
                </div>
            </div>
        </div>
    </section>
    <section class="gallery">
          <div class="wide-container">
          
            <div class="gallery__wrapper">
              <h2>Медиагалерея</h2>
              <div class="gallery__wrapper-items ">
                <div class="gallery__wrapper-item gallery__wrapper-item--one ">
                 <a href="">
                  <img src="./img/img1.webp" alt="" />
                </a>
                </div>
                <div class="gallery__wrapper-item gallery__wrapper-item--two ">
                  <img src="./img/img2.webp" alt="" />
                </div>
                <div class="gallery__wrapper-item ">
                  <img src="./img/img3.webp" alt="" />
                </div>
                <div class="gallery__wrapper-item ">
                  <img src="./img/img4.webp" alt="" />
                </div>
                <div class="gallery__wrapper-item ">
                  <img src="./img/img5.webp" alt="" />
                </div>
                <div class="gallery__wrapper-item ">
                  <img src="./img/img6.webp" alt="" />
                </div>
                <div class="gallery__wrapper-item gallery__wrapper-item--link ">
                  <a href="">
                    <img src="./img/img7.webp" alt="" />
                    <div class="gallery__wrapper-item-text">
                      <p>Перейти в медиагалерею</p>
                      <span>Более 30 фото<br>и видео материалов</sp>
                    </div>
                  </a>
                </div>
              </div>
              <div class="gallery__wrapper-items--mobile"data-mobile="false">
              <div class="swiper__nav">
              <div class="swiper mySwiper ">
                <div class="swiper-wrapper ">
                  <div class="swiper-slide gallery__wrapper-item--mobile-one">
                    <a href="">
                    <img src="./img/img1.webp" alt="" />
                   </a>
                  </div>
                  <div class="swiper-slide"><img src="./img/img2.webp" alt="" /></div>
                  <div class="swiper-slide"><img src="./img/img3.webp" alt="" /></div>
                  <div class="swiper-slide"><img src="./img/img4.webp" alt="" /></div>
                  <div class="swiper-slide"><img src="./img/img5.webp" alt="" /></div>
                  <div class="swiper-slide"><img src="./img/img6.webp" alt="" /></div>
                  <div class="swiper-slide"><a href="">
                    <img src="./img/img7.webp" alt="" />
                   
                   </a>
                  </div>
                </div>
              </div><div class="swiper-pagination"></div>
            </div>
            <a class="gallery__wrapper-items--mobile-btn" href="">Перейти в медиагалерею</a>
          </div>
          
          </div>
        
        </section>
